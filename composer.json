{
    "name": "xypp/collector-view-history",
    "description": "Collector integration with ziven/view-history",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2.0",
        "ziiven/view-history": "*",
        "xypp/collector": "*"
    },
    "authors": [
        {
            "name": "小鱼飘飘",
            "email": "z@xypp.cc",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xypp\\CollectorViewHistory\\": "src/"
        },
        "files": [
            "Overrides/DiscussionViewedHandler.php"
        ]
    },
    "extra": {
        "flarum-extension": {
            "title": "Collector View History",
            "category": "feature",
            "icon": {
                "name": "fas fa-paperclip",
                "color": "#fff",
                "backgroundColor": "#303f9f"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": true,
                "forum": true,
                "js": true,
                "jsCommon": true,
                "css": true,
                "locale": true,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": true,
                "bundlewatch": false,
                "backendTesting": true,
                "editorConfig": true,
                "styleci": true
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
